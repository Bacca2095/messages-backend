{
  "name": "message-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development TZ=UTC nest start",
    "start:dev": "cross-env NODE_ENV=development TZ=UTC nest start --watch",
    "start:debug": "cross-env NODE_ENV=development TZ=UTC nest start --debug --watch",
    "start:prod": "npm run migration:run && node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:run": "cross-env NODE_ENV=development npm run typeorm migration:run -- -d ./data-source.config.ts",
    "migration:generate": "npm run typeorm -- -d ./data-source.config.ts migration:generate ./src/config/database/migrations/$npm_config_name",
    "migration:create": "npm run typeorm -- migration:create ./src/config/database/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm -- -d ./data-source.config.ts migration:revert",
    "migration:drop": "npm run typeorm -- -d ./data-source.config.ts schema:drop"
  },
  "dependencies": {
    "@automapper/classes": "^8.7.7",
    "@automapper/core": "^8.7.7",
    "@automapper/nestjs": "^8.7.7",
    "@automapper/types": "^6.3.1",
    "@hapi/joi": "^17.1.1",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "mysql2": "^3.0.1",
    "nest-winston": "^1.8.0",
    "nestjs-twilio": "^3.2.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.11",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^3.18.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^4.10.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/hapi__joi": "^17.1.9",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.18.0",
    "jest": "28.1.3",
    "prettier": "^2.8.3",
    "sinon": "^15.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.module.ts",
      "!**/*.dto.ts",
      "!**/*.entity.ts",
      "!**/*.profile.ts",
      "!**/main.ts",
      "!**/utils/*.ts",
      "!**/index.ts",
      "!data-source.config.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/src/config/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@auth/(.*)$": [
        "<rootDir>/src/auth/$1"
      ],
      "^@client/(.*)$": [
        "<rootDir>/src/user/$1"
      ],
      "^@config/(.*)$": [
        "<rootDir>/src/config/$1"
      ],
      "^@sms/(.*)$": [
        "<rootDir>/src/sms/$1"
      ],
      "^@user/(.*)$": [
        "<rootDir>/src/user/$1"
      ],
      "^@custom-fields/(.*)$": [
        "<rootDir>/src/custom-fields/$1"
      ],
      "^@contact/(.*)$": [
        "<rootDir>/src/contact/$1"
      ],
      "^@campaign/(.*)$": [
        "<rootDir>/src/campaign/$1"
      ]
    }
  }
}
